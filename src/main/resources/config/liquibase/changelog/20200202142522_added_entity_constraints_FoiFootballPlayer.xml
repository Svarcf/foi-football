<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity FoiFootballPlayer.
    -->
    <changeSet id="20200202142522-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="position_id"
                                 baseTableName="foi_football_player"
                                 constraintName="fk_foi_football_player_position_id"
                                 referencedColumnNames="id"
                                 referencedTableName="foi_football_position"/>

        <addForeignKeyConstraint baseColumnNames="team_id"
                                 baseTableName="foi_football_player"
                                 constraintName="fk_foi_football_player_team_id"
                                 referencedColumnNames="id"
                                 referencedTableName="foi_football_team"/>

    </changeSet>
</databaseChangeLog>
